{
  "name": "dumi-theme-legecy",
  "version": "0.1.7",
  "description": "基于Dumi 2的默认主题修改的dumi 1.x主题",
  "types": "dist/types.d.ts",
  "scripts": {
    "dev": "father dev",
    "build": "father build",
    "prepare": "husky install && father link-dev-theme",
    "lint": "npm run lint:es && npm run lint:css",
    "lint:css": "stylelint \"{src,test}/**/*.{css,less}\"",
    "lint:es": "eslint \"{src,test}/**/*.{js,jsx,ts,tsx}\"",
    "prepublishOnly": "father doctor && npm run build"
  },
  "keywords": [
    "dumi",
    "dumi-theme"
  ],
  "authors": [],
  "license": "MIT",
  "files": [
    "dist"
  ],
  "commitlint": {
    "extends": [
      "gitmoji"
    ]
  },
  "lint-staged": {
    "*.{md,json}": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{css,less}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --parser=typescript --write"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "dumi": "^2.0.2",
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@umijs/lint": "^4.0.44",
    "commitlint": "^17.4.0",
    "commitlint-config-gitmoji": "^2.2.8",
    "dumi": "^2.1.7",
    "eslint": "^8.23.0",
    "father": "^4.1.0",
    "father-plugin-dumi-theme": "^1.0.0-beta.0",
    "husky": "^8.0.1",
    "icons-svg": "link:@types/@ant-design/icons-svg",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "^3.0.0",
    "prettier-plugin-packagejson": "^2.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.0.0",
    "stylelint": "^14.9.1"
  },
  "dependencies": {
    "@ant-design/icons-svg": "^4.2.1",
    "@babel/runtime": "^7.20.7",
    "@makotot/ghostui": "^2.0.0",
    "animated-scroll-to": "^2.3.0",
    "classnames": "^2.3.2",
    "heti": "^0.9.2",
    "lodash.throttle": "^4.1.1",
    "prism-react-renderer": "^1.3.5",
    "prism-themes": "^1.9.0",
    "rc-tabs": "^12.5.5",
    "react-copy-to-clipboard": "^5.1.0",
    "react-intl": "^6.2.7"
  }
}
